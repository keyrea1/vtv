{
  "name": "VTV-shell",
  "version": "1.0.0",
  "description": "VTV-Shell application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "_start": "webpack-dev-server --host 0.0.0.0",
    "lint": "eslint sources/",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "build": "webpack --env.production true",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/png/zvo-logo-vector.png --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/png/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "repository": "https://bitbucket.org/datacomteam/vtv-shell",
  "keywords": [
    "Electron",
    "VTV",
    "ZVO"
  ],
  "author": "IDEAStudio",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "check-for-leaks": "^1.2.0",
    "css-loader": "^0.28.7",
    "devtron": "^1.3.0",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^2.2.0",
    "eslint": "^4.7.2",
    "file-loader": "^0.11.2",
    "husky": "^0.14.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "spectron": "^5.0.0",
    "standard": "^8.2.0",
    "url-loader": "^0.5.9",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electromon": "^1.0.10",
    "electron": "^3.0.3",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "executive": "^1.6.3",
    "glob": "^7.1.0",
    "highlight.js": "^9.3.0",
    "nodemon": "^1.18.9",
    "update-electron-app": "^1.1.1",
    "webix-jet": "^1.6.2",
    "xmlhttprequest": "^1.8.0"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
